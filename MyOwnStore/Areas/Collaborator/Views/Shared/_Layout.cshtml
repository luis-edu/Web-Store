@inject MyOwnStore.Libraries.Login.LoginCollaborator lCollaborator
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!-- Créditos do desenvolvedor do template-->
    <meta name="keywords" content="HTML5 Admin Template" />
    <meta name="description" content="JSOFT Admin - Responsive HTML5 Template">
    <meta name="author" content="JSOFT.net">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap-datepicker/css/datepicker3.css" />

    <!-- Specific Page Vendor CSS -->
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap-tagsinput/bootstrap-tagsinput.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/summernote/summernote.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/summernote/summernote-bs3.css" />

    <!--Specific Forms Vendor CSS-->
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.custom.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/select2/select2.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap-multiselect/bootstrap-multiselect.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap-tagsinput/bootstrap-tagsinput.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap-colorpicker/css/bootstrap-colorpicker.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap-timepicker/css/bootstrap-timepicker.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/dropzone/css/basic.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/dropzone/css/dropzone.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/bootstrap-markdown/css/bootstrap-markdown.min.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/summernote/summernote.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/summernote/summernote-bs3.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/codemirror/lib/codemirror.css" />
    <link rel="stylesheet" href="~/DashboardResources/assets/vendor/codemirror/theme/monokai.css" />


    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/DashboardResources/assets/stylesheets/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="~/DashboardResources/assets/stylesheets/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/DashboardResources/assets/stylesheets/theme-custom.css">

    <!-- Head Libs -->
    <script src="~/DashboardResources/assets/vendor/modernizr/modernizr.js"></script>
    

    <style>
        .color-title {
            color: #abb4be !important
        }
    </style>

</head>
<body>
    <section class="body">

        <!-- start: header -->
        <header class="header">
            <div class="logo-container">
                <a href="../" class="logo">
                    <img src="~/images/logo.svg" height="35" alt="JSOFT Admin" />
                </a>
                <div class="visible-xs toggle-sidebar-left" data-toggle-class="sidebar-left-opened" data-target="html" data-fire-event="sidebar-left-opened">
                    <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                </div>
            </div>

            <!-- start: search & user box -->
            <div class="header-right">

                <form action="pages-search-results.html" class="search nav-form">
                    <div class="input-group input-search">
                        <input type="text" class="form-control" name="q" id="q" placeholder="Search...">
                        <span class="input-group-btn">
                            <button class="btn btn-default" type="submit"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                </form>

                <span class="separator"></span>

                <ul class="notifications">
                    <li>
                        <a href="#" class="dropdown-toggle notification-icon" data-toggle="dropdown">
                            <i class="fa fa-tasks"></i>
                            <span class="badge">3</span>
                        </a>

                        <div class="dropdown-menu notification-menu large">
                            <div class="notification-title">
                                <span class="pull-right label label-default">3</span>
                                Taréfas
                            </div>

                            <div class="content">
                                <ul>
                                    <li>
                                        <p class="clearfix mb-xs">
                                            <span class="message pull-left">Desenvolvimento do dashboard</span>
                                            <span class="message pull-right text-dark">10%</span>
                                        </p>
                                        <div class="progress progress-xs light">
                                            <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 10%;"></div>
                                        </div>
                                    </li>

                                    <li>
                                        <p class="clearfix mb-xs">
                                            <span class="message pull-left">Desenvolvimento do Site</span>
                                            <span class="message pull-right text-dark">5%</span>
                                        </p>
                                        <div class="progress progress-xs light">
                                            <div class="progress-bar" role="progressbar" aria-valuenow="98" aria-valuemin="0" aria-valuemax="100" style="width: 5%;"></div>
                                        </div>
                                    </li>

                                    <li>
                                        <p class="clearfix mb-xs">
                                            <span class="message pull-left">Alguma coisa</span>
                                            <span class="message pull-right text-dark">0%</span>
                                        </p>
                                        <div class="progress progress-xs light mb-xs">
                                            <div class="progress-bar" role="progressbar" aria-valuenow="33" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"></div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle notification-icon" data-toggle="dropdown">
                            <i class="fa fa-envelope"></i>
                            <span class="badge">4</span>
                        </a>

                        <div class="dropdown-menu notification-menu">
                            <div class="notification-title">
                                <span class="pull-right label label-default">3</span>
                                Mensagens
                            </div>

                            <div class="content">
                                <ul>
                                    <li>
                                        <a href="#" class="clearfix">
                                            <figure class="image">
                                                <img src="~/DashboardResources/assets/images/!sample-user.jpg" alt="Joseph Doe Junior" class="img-circle" />
                                            </figure>
                                            <span class="title">Joseph Doe</span>
                                            <span class="message">Lorem ipsum dolor sit.</span>
                                        </a>
                                    </li>

                                    <li>
                                        <a href="#" class="clearfix">
                                            <figure class="image">
                                                <img src="~/DashboardResources/assets/images/!sample-user.jpg" alt="Joe Junior" class="img-circle" />
                                            </figure>
                                            <span class="title">Joe Junior</span>
                                            <span class="message">Lorem ipsum dolor sit.</span>
                                        </a>
                                    </li>
                                </ul>

                                <hr />

                                <div class="text-right">
                                    <a href="#" class="view-more">Ver todas</a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle notification-icon" data-toggle="dropdown">
                            <i class="fa fa-bell"></i>
                            <span class="badge">3</span>
                        </a>

                        <div class="dropdown-menu notification-menu">
                            <div class="notification-title">
                                <span class="pull-right label label-default">3</span>
                                Alertas
                            </div>

                            <div class="content">
                                <ul>
                                    <li>
                                        <a href="#" class="clearfix">
                                            <div class="image">
                                                <i class="fa fa-thumbs-down bg-danger"></i>
                                            </div>
                                            <span class="title">Server is Down!</span>
                                            <span class="message">Just now</span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#" class="clearfix">
                                            <div class="image">
                                                <i class="fa fa-lock bg-warning"></i>
                                            </div>
                                            <span class="title">User Locked</span>
                                            <span class="message">15 minutes ago</span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#" class="clearfix">
                                            <div class="image">
                                                <i class="fa fa-signal bg-success"></i>
                                            </div>
                                            <span class="title">Connection Restaured</span>
                                            <span class="message">10/10/2014</span>
                                        </a>
                                    </li>
                                </ul>

                                <hr />

                                <div class="text-right">
                                    <a href="#" class="view-more">Ver todos</a>
                                </div>
                            </div>
                        </div>
                    </li>
                </ul>

                <span class="separator"></span>

                <div id="userbox" class="userbox">
                    <a href="#" data-toggle="dropdown">
                        <figure class="profile-picture">
                            <img src="~/DashboardResources/assets/images/!logged-user.jpg" alt="Joseph Doe" class="img-circle" data-lock-picture="assets/images/!logged-user.jpg" />
                        </figure>
                        <div class="profile-info" data-lock-name="John Doe" data-lock-email="johndoe@JSOFT.com">
                            <span class="name"></span>
                            <span class="role"></span>
                        </div>

                        <i class="fa custom-caret"></i>
                    </a>

                    <div class="dropdown-menu">
                        <ul class="list-unstyled">
                            <li class="divider"></li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="pages-user-profile.html"><i class="fa fa-user"></i> My Profile</a>
                            </li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="#" data-lock-screen="true"><i class="fa fa-lock"></i> Lock Screen</a>
                            </li>
                            <li>
                                <a role="menuitem" tabindex="-1" asp-action="Logout" asp-controller="Home"><i class="fa fa-power-off"></i> Logout</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- end: search & user box -->
        </header>
        <!-- end: header -->

        <div class="inner-wrapper">
            <!-- start: sidebar -->
            <aside id="sidebar-left" class="sidebar-left">

                <div class="sidebar-header">
                    <div class="sidebar-title color-title">
                        Navigation
                    </div>
                    <div class="sidebar-toggle hidden-xs" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                        <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                    </div>
                </div>

                <div class="nano has-scrollbar">
                    <div class="nano-content" tabindex="0" style="right: -17px;">
                        <nav id="menu" class="nav-main" role="navigation">
                            <ul class="nav nav-main">
                                <li class="nav-active">
                                    <a asp-action="Panel" asp-controller="Home">
                                        <i class="fa fa-home" aria-hidden="true"></i>
                                        <span>Dashboard</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-action="Index" asp-controller="Category">
                                        <i class="fa fa-home" aria-hidden="true"></i>
                                        <span>Categorias</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-action="Index" asp-controller="Product">
                                        <i class="fa fa-home" aria-hidden="true"></i>
                                        <span>Produtos</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-action="Index" asp-controller="Client">
                                        <i class="fa fa-home" aria-hidden="true"></i>
                                        <span>Clientes</span>
                                    </a>
                                </li>
                                @if (!false)
                                {
                                    <li>
                                        <a asp-action="Index" asp-controller="Collaborator">
                                            <i class="fa fa-person-booth" aria-hidden="true"></i>
                                            <span>Colaboradores</span>
                                        </a>
                                    </li>
                                }
                                <li>
                                    <a class="disabled">
                                        <span class="pull-right label label-primary">182</span>
                                        <i class="fa fa-envelope" aria-hidden="true"></i>
                                        <span>Emails <em class="not-included">(Em breve!)</em></span>
                                    </a>
                                </li>
                                <li class="nav-parent">
                                    <a>
                                        <i class="fa fa-copy" aria-hidden="true"></i>
                                        <span>Menu</span>
                                    </a>
                                    <ul class="nav nav-children">
                                        <li>
                                            <a>
                                                Submenu
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>

                        <hr class="separator" />

                        <div class="sidebar-widget widget-tasks">
                            <div class="widget-header">
                                <h6 class="color-title">Projetos</h6>
                                <div class="widget-toggle">+</div>
                            </div>
                            <div class="widget-content">
                                <ul class="list-unstyled m-none">
                                    <li><a href="#">Projeto 1</a></li>
                                    <li><a href="#">Projeto 2</a></li>
                                    <li><a href="#">Projeto 3</a></li>
                                </ul>
                            </div>
                        </div>

                        <hr class="separator" />

                        <div class="sidebar-widget widget-stats">
                            <div class="widget-header">
                                <h6 class="color-title">Status da compania</h6>
                                <div class="widget-toggle">+</div>
                            </div>
                            <div class="widget-content">
                                <ul>
                                    <li>
                                        <span class="stats-title">Stat 1</span>
                                        <span class="stats-complete color-title">85%</span>
                                        <div class="progress">
                                            <div class="progress-bar progress-bar-primary progress-without-number" role="progressbar" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100" style="width: 85%;">
                                                <span class="sr-only">85% Complete</span>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <span class="stats-title">Stat 2</span>
                                        <span class="stats-complete color-title">70%</span>
                                        <div class="progress">
                                            <div class="progress-bar progress-bar-primary progress-without-number" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width: 70%;">
                                                <span class="sr-only">70% Complete</span>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <span class="stats-title">Stat 3</span>
                                        <span class="stats-complete color-title">2%</span>
                                        <div class="progress">
                                            <div class="progress-bar progress-bar-primary progress-without-number" role="progressbar" aria-valuenow="2" aria-valuemin="0" aria-valuemax="100" style="width: 2%;">
                                                <span class="sr-only">2% Complete</span>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>

                </div>

            </aside>
            <!-- end: sidebar -->
            @RenderBody()
            <aside id="sidebar-right" class="sidebar-right">
                <div class="nano">
                    <div class="nano-content">
                        <a href="#" class="mobile-close visible-xs">
                            Collapse <i class="fa fa-chevron-right"></i>
                        </a>

                        <div class="sidebar-right-wrapper">

                            <div class="sidebar-widget widget-calendar">
                                <h6>Upcoming Tasks</h6>
                                <div data-plugin-datepicker data-plugin-skin="dark"></div>

                                <ul>
                                    <li>
                                        <time datetime="2014-04-19T00:00+00:00">04/19/2014</time>
                                        <span>Company SuckMahdick</span>
                                    </li>
                                </ul>
                            </div>

                            <div class="sidebar-widget widget-friends">
                                <h6>Friends</h6>
                                <ul>
                                    <li class="status-online">
                                        <figure class="profile-picture">
                                            <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                        </figure>
                                        <div class="profile-info">
                                            <span class="name">Joseph Doe Junior</span>
                                            <span class="title">Hey, how are you?</span>
                                        </div>
                                    </li>
                                    <li class="status-online">
                                        <figure class="profile-picture">
                                            <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                        </figure>
                                        <div class="profile-info">
                                            <span class="name">Joseph Doe Junior</span>
                                            <span class="title">Hey, how are you?</span>
                                        </div>
                                    </li>
                                    <li class="status-offline">
                                        <figure class="profile-picture">
                                            <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                        </figure>
                                        <div class="profile-info">
                                            <span class="name">Joseph Doe Junior</span>
                                            <span class="title">Hey, how are you?</span>
                                        </div>
                                    </li>
                                    <li class="status-offline">
                                        <figure class="profile-picture">
                                            <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                        </figure>
                                        <div class="profile-info">
                                            <span class="name">Joseph Doe Junior</span>
                                            <span class="title">Hey, how are you?</span>
                                        </div>
                                    </li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>
            </aside>
        </div>
    </section>



    <!-- Vendor -->
    <script src="~/DashboardResources/assets/vendor/jquery/jquery.js"></script>
    <script src="~/DashboardResources/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/DashboardResources/assets/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/DashboardResources/assets/vendor/magnific-popup/magnific-popup.js"></script>
    <script src="~/DashboardResources/assets/vendor/jquery-placeholder/jquery.placeholder.js"></script>

    <!-- Specific Page Vendor -->
    <script src="~/DashboardResources/assets/vendor/bootstrap-tagsinput/bootstrap-tagsinput.js"></script>
    <script src="~/DashboardResources/assets/vendor/summernote/summernote.js"></script>


    <!-- Forms Specific Vender-->
    <script src="~/DashboardResources/assets/vendor/jquery-ui/js/jquery-ui-1.10.4.custom.js"></script>
    <script src="~/DashboardResources/assets/vendor/jquery-ui-touch-punch/jquery.ui.touch-punch.js"></script>
    <script src="~/DashboardResources/assets/vendor/select2/select2.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-multiselect/bootstrap-multiselect.js"></script>
    <script src="~/DashboardResources/assets/javascripts/jquery.mask.min.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-tagsinput/bootstrap-tagsinput.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-colorpicker/js/bootstrap-colorpicker.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-timepicker/js/bootstrap-timepicker.js"></script>
    <script src="~/DashboardResources/assets/vendor/fuelux/js/spinner.js"></script>
    <script src="~/DashboardResources/assets/vendor/dropzone/dropzone.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-markdown/js/markdown.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-markdown/js/to-markdown.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-markdown/js/bootstrap-markdown.js"></script>
    <script src="~/DashboardResources/assets/vendor/codemirror/lib/codemirror.js"></script>
    <script src="~/DashboardResources/assets/vendor/codemirror/addon/selection/active-line.js"></script>
    <script src="~/DashboardResources/assets/vendor/codemirror/addon/edit/matchbrackets.js"></script>
    <script src="~/DashboardResources/assets/vendor/codemirror/mode/javascript/javascript.js"></script>
    <script src="~/DashboardResources/assets/vendor/codemirror/mode/xml/xml.js"></script>
    <script src="~/DashboardResources/assets/vendor/codemirror/mode/htmlmixed/htmlmixed.js"></script>
    <script src="~/DashboardResources/assets/vendor/codemirror/mode/css/css.js"></script>
    <script src="~/DashboardResources/assets/vendor/summernote/summernote.js"></script>
    <script src="~/DashboardResources/assets/vendor/bootstrap-maxlength/bootstrap-maxlength.js"></script>
    <script src="~/DashboardResources/assets/vendor/ios7-switch/ios7-switch.js"></script>

    <!--Validation Specific Vendor-->
    <script src="~/DashboardResources/assets/assets/vendor/jquery-validation/jquery.validate.js"></script>
    <script src="~/DashboardResources/assets/javascripts/forms/examples.validation.js"></script>

    <!-- Theme Base, Components and Settings -->
    <script src="~/DashboardResources/assets/javascripts/theme.js"></script>

    <!-- Theme Custom -->
    <script src="~/DashboardResources/assets/javascripts/theme.custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="~/DashboardResources/assets/javascripts/theme.init.js"></script>
    <script src="~/DashboardResources/assets/javascripts/Actions.js"></script>


</body>
</html>
